CREATE TABLE `individual_acc` (
  `c_code` int NOT NULL AUTO_INCREMENT,
  `c_name` varchar(45) NOT NULL,
  `c_father_name` varchar(45) NOT NULL,
  `c_mother_name` varchar(45) NOT NULL,
  `c_account_type` varchar(45) DEFAULT NULL,
  `c_opening_date` date DEFAULT NULL,
  `c_dob` date NOT NULL,
  `c_sex` varchar(10) NOT NULL,
  `c_pre_add` varchar(200) DEFAULT NULL,
  `c_pre_city` varchar(300) DEFAULT NULL,
  `c_pre_post_code` varchar(300) DEFAULT NULL,
  `c_pre_country` varchar(300) DEFAULT NULL,
  `c_pre_tel` varchar(45) DEFAULT NULL,
  `c_per_add` varchar(200) DEFAULT NULL,
  `c_per_city` varchar(400) DEFAULT NULL,
  `c_per_post_code` varchar(45) DEFAULT NULL,
  `c_per_country` varchar(45) DEFAULT NULL,
  `c_per_tel` varchar(45) DEFAULT NULL,
  `c_type` varchar(20) NOT NULL,
  `c_division` varchar(45) NOT NULL,
  `c_mobile_num` varchar(45) DEFAULT NULL,
  `c_email` varchar(45) DEFAULT NULL,
  `c_occupation` varchar(45) DEFAULT NULL,
  `c_etin` int NOT NULL,
  `c_nationality` varchar(45) DEFAULT NULL,
  `c_branch` varchar(45) DEFAULT NULL,
  `c_bank_routing_num` int DEFAULT NULL,
  `c_bank_account_num` int DEFAULT NULL,
  `c_bank_branch_name` varchar(45) DEFAULT NULL,
  `c_passport_num` varchar(45) DEFAULT NULL,
  `c_operation_type` varchar(45) DEFAULT NULL,
  `introducer_id` int DEFAULT NULL,
  `bo_id` int DEFAULT NULL,
  `nom_id` int DEFAULT NULL,
  `poa_id` int DEFAULT NULL,
  `internet_trading` char(1) DEFAULT NULL,
  `dse` char(1) DEFAULT NULL,
  `cse` char(1) DEFAULT NULL,
  `oms` char(1) DEFAULT NULL,
  `sms_service` char(1) DEFAULT NULL,
  `email_service` char(1) DEFAULT NULL,
  PRIMARY KEY (`c_code`),
  UNIQUE KEY `c_code_UNIQUE` (`c_code`),
  UNIQUE KEY `c_etin_UNIQUE` (`c_etin`),
  UNIQUE KEY `c_bank_account_num_UNIQUE` (`c_bank_account_num`),
  KEY `introducer_id_idx` (`introducer_id`)
) ENGINE=InnoDB AUTO_INCREMENT=88 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci
